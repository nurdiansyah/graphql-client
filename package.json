{
  "name": "@deboxsoft/graphql-client",
  "version": "0.4.5",
  "description": "",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "lint": "npm run eslint --fix",
    "check": "npm run lint && tsc --noEmit",
    "clean": "rimraf libs",
    "build": "npm run clean && tsc && cpx package.json ./libs",
    "build:typings": "tsc -p tsconfig.typings.json",
    "build:dev": "npm run clean && tsc --watch",
    "publish:package": "npm run build && cd libs && npm publish --access public",
    "test": "cross-env NODE_ENV=test jest src",
    "test:watch": "npm run test --watch",
    "test:coverage": "npm run test --coverage"
  },
  "author": "Nurdiansyah",
  "homepage": "https://github.com/nurdiansyah/graphql-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nurdiansyah/graphql-client.git"
  },
  "jest": {
    "preset": "@deboxsoft/devel-react",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "tslint --fix --project .",
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "apollo-client": "*",
    "graphql": "*",
    "react": "^16.8.0"
  },
  "dependencies": {
    "eslint": "^5.16.0",
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@deboxsoft/babel-preset-devel": "^2.0.8",
    "@deboxsoft/devel": "^2.1.4",
    "@deboxsoft/devel-react": "^1.0.6",
    "@deboxsoft/devel-typescript": "^0.0.14",
    "@deboxsoft/eslint-config-devel": "^1.0.11",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.10",
    "@types/lodash.isplainobject": "4.0.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.6.0-beta.8",
    "apollo-link": "^1.2.11",
    "apollo-link-mock": "^1.0.1",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-dom": "^2.1.1",
    "jest-react-profiler": "^0.1.3",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-testing-library": "^5.5.3",
    "standard-version": "^4.4.0",
    "typescript": "~3.3.0"
  }
}
